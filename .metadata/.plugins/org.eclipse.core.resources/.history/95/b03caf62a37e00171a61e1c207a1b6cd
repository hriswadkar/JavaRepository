package com.selftest.chapter6;

/**
 * @author harsh
 *
 */
public class Flipper {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String o = "-";
		switch("FRED".toLowerCase().substring(1, 3)) {
		case "yellow":
			o += "y";
		case "red":
			o += "r";
		case "green":
			o += "g";
		}
		
		System.out.println(o);
	}

}
